#include "src\version.h"
#include <winres.h>

1 VERSIONINFO
 FILEVERSION CEXT_VERSION_MAJOR, CEXT_VERSION_MINOR, CEXT_VERSION_PATCH, CEXT_VERSION_BETA
 PRODUCTVERSION CEXT_VERSION_MAJOR, CEXT_VERSION_MINOR, CEXT_VERSION_PATCH, CEXT_VERSION_BETA
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "CCExtenderF4"
			VALUE "FileVersion", CEXT_VERSION_VERSTRING
            VALUE "InternalName", "CCExtenderF4"
            VALUE "LegalCopyright", "MIT License"
            VALUE "ProductName", "CCExtenderF4"
			VALUE "ProductVersion", CEXT_VERSION_VERSTRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
